#FindUsers
  input
    width: 100%;
    background: #7a0c0b;
    padding: 0px 30px;
    border: 0px;
    height: 70px;
    margin-bottom: 20px;
    border-radius: 5px;
    transition 0.1s
    &:focus
      background white
      color #333
  .User
    display: inline-table;
    margin: 5px 5px 5px 0px
    line-height: 50px;
    background: #7c0c0b;
    padding: 10px;
    border-radius: 5px;
  .ProfilePicture
    width 50px
    height 50px
    margin-right 10px
    float left
  a:hover
    border-bottom 1px solid
