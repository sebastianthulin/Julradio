#Schedule
  margin-bottom 20px
  text-align center
  border-radius 5px
  overflow hidden
  &.expanded section
    display block
  section
    padding 20px 0
    display none
    &:first-child
      display block
    &:nth-child(odd)
      background rgba(245, 55, 55, 0.2)
    &:nth-child(even)
      background rgba(230, 103, 103, 0.11)
  footer
    padding 10px 0
    background rgba(245, 55, 55, 0.2)
    span
      cursor pointer
      noUserSelect()

  header
    display table
    margin 0 auto
    padding-bottom 5px
    font-size 22px
    font-weight 600
    border-bottom 2px solid rgba(255, 255, 255, 0.5)

.Reservation
  margin-top 20px
  padding 0 10px
  font-weight 400
  line-height 1.4
  a:hover
    border-bottom 1px solid
  .description
    color rgba(255, 255, 255, 0.6)

#Front
  .compose
    margin-bottom 10px
    padding 17px
    color #454545
    background #FFF
    border-radius 3px
    noUserSelect()

  .informer
    font-size 13px
    font-style italic
    color rgba(255, 255, 255, 0.67)
    width 100%
    text-align center
    margin -5px 0px 10px 0px
    display block

  .article
    margin-bottom 30px
    padding-bottom 30px
    border-bottom 2px solid rgba(255, 255, 255, 0.3)
    &:last-child
      border-bottom none

  .commentLink
    display inline-flex
    align-items center
    color rgba(255, 255, 255, 0.76)
    &:hover svg g
      stroke #FFF
    svg
      width 22px
      height 24px
      margin-right 8px
      g
        stroke @color
        transition stroke .1s ease-in-out
          
  .facerurubohorukuru
    position relative
    display block
    width 100%
    margin-top 10px
    padding 4px
    z-index 0
    border-radius 5px
    background #FFF
    .boxie
      position relative
      height 58px
      padding 15px
      border 2px solid #3B5998
      
    .textie
      position absolute
      top 23px
      left 15px
      padding 0px 5px
      z-index 1
      font-weight 400
      border-radius 3px
      color #3B5998
      background #FFF
      
    img
      position absolute
      width 80px
      right 11px
      top -14px


  .sponsors
    margin-top 50px
    a
      display block
      margin 30px 0
      text-align center
      img
        width 100%
        max-width 250px

.Feed
  max-height 1000px
  overflow-y scroll
  border-radius 3px

.Request
  position relative
  padding 10px
  background rgba(0, 0, 0, 0.15)
  margin-bottom 10px
  border-radius 3px
  overflow hidden

  
  img
    margin-right 10px
    border-radius 50%
    width 30px
    float left
    
  .user
    color #FFF
    font-weight 400
    height 30px
    line-height 30px
  a:hover
    border-bottom 1px solid
    
  .text
    display block
    color rgba(255,255,255,0.7)
    margin 5px 0 0 0
  
  .song
    margin-top 5px
    color rgba(255, 255, 255, 0.4)

  .remove
    position absolute
    top 4px
    right 10px
    color #FFF
    background none
    opacity 0.2
    cursor pointer
    &:hover
      opacity 0.5