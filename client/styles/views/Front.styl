#Front
  #Schedule
    margin-bottom 20px
    text-align center
    background rgba(230, 103, 103, 0.11)
    padding 20px 0
    border-radius 5px
    :last-child
      margin-bottom 0

    h3
      font-size 33px
      margin-bottom 0

    p
      //

  .Article
    margin-bottom 30px
    padding-bottom 30px
    border-bottom 2px solid rgba(255, 255, 255, 0.6)
    .header
      height 70px
      line-height 38px
      img
        float left
        width 50px
        height 50px
        margin-right 10px
      .user
        color #FFF
        text-decoration none
        &:hover
          border-bottom 1px solid
      .timestamp
        color rgba(255,255,255,0.53)
        font-size 11px
        margin-top -9px
        display block
        line-height 10px

  .compose
    position relative
    margin-bottom 10px
    padding 10px
    color #000
    background #FFF
    border-radius 3px
    noUserSelect()

  .TwitterFeed
    max-height 1000px

  .informer
    font-size 13px
    font-style italic
    color rgba(255, 255, 255, 0.67)
    width 100%
    text-align center
    margin -5px 0px 10px 0px
    display block