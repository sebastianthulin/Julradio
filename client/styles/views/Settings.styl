#settings
  .settingsInput
    height 40px
    line-height 42px
    padding 0px 10px
    border-radius 3px
    margin-bottom 10px
    width 230px
    
  .settingsSave
    height 40px
    line-height 42px
    padding 0px 10px
    border-radius 3px
    width 230px
    background white
    
  .settingsInputArea
    min-height 100px
    line-height 16px
    padding-top 10px
    margin-bottom 2px
    resize vertical
    
  input[type="file"]
    color white
    line-height 16px

  h6
    margin 10px 0px 0px 0px
    
  h4
    margin 0px 0px 10px 0px