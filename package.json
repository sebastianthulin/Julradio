{
  "name": "julradio",
  "version": "4.1.0",
  "scripts": {
    "dev": "concurrently --kill-others 'npm run server' 'npm run webpack'",
    "server": "cd server && nodemon --exec babel-node index.js",
    "build": "cd client && NODE_ENV=production webpack",
    "webpack": "cd client && webpack-dev-server --inline --hot --host 0.0.0.0 --colors --no-info"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "concurrently": "^2.2.0",
    "css-loader": "^0.24.0",
    "nodemon": "^1.10.2",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
